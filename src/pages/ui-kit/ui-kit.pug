block variables
doctype html
html(lang='ru')
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='description', content=description)
    meta(name='keywords', content=keywords)
    title #{title}
  body
    header.ui-kit-header
      img(
        src=require('../../img/logo-bw.svg'),
        alt='Toxin Logo'
      )
    block content
